  "type": "module",
  "description": "Genesis Luminal - Frontend React com experiÃªncia transcendental",
  "keywords": [
    "digital-consciousness",
    "emotional-ai",
    "morphogenesis",
    "quantum-sync",
    "three.js",
    "tensorflow.js",
    "transcendental-ux"
  ],
  "author": "Genesis Luminal Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "perf": "npm run dev --performance",
    "debug": "npm run dev --debug",
    "optimize": "npm run build -- --mode=production",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "audit:security": "npm audit --audit-level high || echo 'Vulnerabilities found - see report above'",
    "audit:licenses": "license-checker --summary",
    "audit:outdated": "npm outdated",
    "audit:full": "../scripts/audit-dependencies.sh",
    "generate:sbom": "../scripts/generate-sbom.sh",
    "check:secrets": "echo \"Checking for secrets...\" && grep -r \"sk-\" src/ || echo \"No secrets found\"",
    "security:check": "npm run audit:security && npm run check:secrets",
    "compliance:check": "npm run audit:licenses && npm run audit:outdated",
    "audit:info": "npm audit --audit-level high || true",
    "test:ci": "vitest run --coverage"
  },
