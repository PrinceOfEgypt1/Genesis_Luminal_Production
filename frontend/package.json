    "ml-kmeans": "^6.0.0",
    "ml-matrix": "^6.10.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.153.0",
    "tone": "^14.9.17",
    "typescript": "^5.0.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.16.0",
